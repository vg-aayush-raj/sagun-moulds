  "type": "module",
  "scripts": {
    "dev": "vite --mode=dev",
    "build": "pnpm run type-check && pnpm run lint && vite build --mode",
    "type-check": "tsc -b",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preinstall": "npx -y only-allow pnpm",
    "preview": "vite preview",
    "prepare": "husky",
    "scan": "react-scan http://localhost:<your-port>",
