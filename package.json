    "@mui/x-date-pickers": "^7.29.4",
    "axios": "^1.7.9",
    "date-fns": "^3.0.0",
    "dayjs": "^1.11.19",
    "framer-motion": "^12.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.4",
    "react-router-dom": "^7.1.3",
    "zod": "^3.25.28"
